public class Filme {
	String nome;
	int anoLancamento;
	boolean incluidoAssinatura;
	double avaliacao;
	int totalAvaliacao;
	int duracaoMinutos;
	
	void mostraDados() {
		System.out.println("Nome: " + nome);
		System.out.println("Lançado em: " + anoLancamento);
		System.out.println("Duração: " + duracaoMinutos + " minutos");
	}
}
